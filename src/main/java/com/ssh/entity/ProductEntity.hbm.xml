<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.ProductEntity" table="product" schema="sale">
        <id name="proId" column="pro_id"/>
        <property name="proName" column="pro_name"/>
        <property name="proImg" column="pro_img"/>
        <property name="proPrice" column="pro_price"/>
        <property name="date" column="date"/>
    </class>
</hibernate-mapping>